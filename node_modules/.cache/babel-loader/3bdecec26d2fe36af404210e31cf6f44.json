A,CAAA,OAAA,GAAA,IAAA;AACA,UAAA,YAAA,CAAA,OAAA,CAAA,iBAAA,EAAA,GAAA;AACA,SAHA;AAIA,OALA,MAKA;AACA,YAAA,GAAA,GAAA,cAAA;;AACA,YAAA,aAAA,KAAA,GAAA,EAAA;AACA,UAAA,GAAA,GAAA,iBAAA;AACA,SAFA,MAEA,IAAA,aAAA,CAAA,QAAA,OAAA,MAAA,EAAA;AACA,UAAA,GAAA,GAAA,kBAAA;AACA,SAFA,MAEA,IAAA,aAAA,KAAA,MAAA,EAAA;AACA,UAAA,GAAA,GAAA,yBAAA;AACA;;AACA,QAAA,MAAA,CAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA,CAAA;AACA,aAAA,KAAA,GAAA,KAAA,KAAA,GAAA,CAAA;AACA,QAAA,YAAA,CAAA,OAAA,CACA,MADA,EACA,IAAA,CAAA,SAAA,CAAA;AACA,UAAA,KAAA,EAAA,KAAA,KADA;AAEA,UAAA,SAAA,EAAA,IAAA,IAAA,GAAA,OAAA;AAFA,SAAA,CADA;AAMA;;AACA,WAAA,KAAA,GAAA,EAAA;AACA,KAjCA;AAkCA,IAAA,IAlCA,kBAkCA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA;AADA,OAAA;AAGA,KAtCA;AAuCA,IAAA,MAvCA,oBAuCA;AACA,WAAA,SAAA,GAAA,IAAA;AACA;AAzCA,GAxBA;AAmEA,EAAA,OAnEA,qBAmEA;AACA,QAAA,IAAA,GAAA,YAAA,CAAA,OAAA,CAAA,MAAA,CAAA;AACA,QAAA,SAAA,GAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,GAAA,IAAA;AACA,QAAA,SAAA,GAAA,SAAA,GAAA,SAAA,CAAA,SAAA,GAAA,CAAA;AACA,QAAA,aAAA,GAAA,IAAA,IAAA,GAAA,OAAA,EAAA;AACA,QAAA,KAAA,GAAA,SAAA,GAAA,SAAA,CAAA,KAAA,GAAA,CAAA;AACA,QAAA,MAAA,GAAA,IAAA,IAAA,CAAA,IAAA,IAAA,GAAA,kBAAA,EAAA,EAAA,OAAA,KAAA,IAAA,EAAA,GAAA,EAAA,GAAA,IAAA;AACA,SAAA,KAAA,GACA,aAAA,GAAA,MAAA,GAAA,KAAA,GAAA,SAAA,GAAA,MAAA,GAAA,KAAA,GAAA,CADA;AAEA,QAAA,OAAA,GAAA,IAAA,IAAA,GAAA,OAAA,EAAA;AACA,QAAA,aAAA,GAAA,OAAA,GAAA,aAAA;AACA,SAAA,OAAA,GACA,YAAA,CAAA,OAAA,CAAA,iBAAA,IACA,YAAA,CAAA,OAAA,CAAA,iBAAA,CADA,GAEA,aAAA,GACA,aADA,GAEA,KALA;AAMA;AApFA,CAAA","sourcesContent":["<template>\n  <div class=\"hello\">\n    <div v-if=\"!success\" class=\"display_flex\">\n      <Expecting></Expecting>\n      <p>您的礼物，火速送达中~</p>\n      <VanButton plain type=\"primary\" @click=\"working\">良辰吉日，自动揭晓</VanButton>\n      <VanButton\n          class=\"not_waiting\"\n          @click=\"imagine\"\n          plain\n          :disabled=\"times < 1\"\n          type=\"warning\">急不可待，芝麻开门</VanButton>\n      <VanButton type=\"info\" class=\"not_waiting\" plain @click=\"play\">小试牛刀，密码碎片</VanButton>\n      <VanButton type=\"danger\" class=\"not_waiting\" plain @click=\"qrcode\">专业顾问，分析解码</VanButton>\n      <VanDialog v-model=\"show\" :title=\"title\" show-cancel-button @confirm=\"confirm\">\n        <van-field\n            v-model=\"value\"\n            placeholder=\"纯数字密码，长度未知\"\n            input-align=\"center\"\n            type=\"number\"\n        />\n      </VanDialog>\n      <van-popup v-model=\"popUpShow\">\n        <p style=\"color: #1989fa;\">微信扫码获取您的私人顾问~</p>\n        <van-image\n            width=\"300\"\n            :src=\"qrcodeImg\"\n        />\n      </van-popup>\n    </div>\n    <Result v-else></Result>\n  </div>\n</template>\n\n<script>\nimport { Dialog } from 'vant';\nimport Expecting from \"../components/Expecting\";\nimport Result from \"../components/result\";\n\nexport default {\n  name: 'HelloWorld',\n  components: {\n    Expecting, Result\n  },\n  props: {\n    msg: String\n  },\n  data () {\n    return {\n      show: false,\n      value: \"\",\n      times: 5,\n      success: false,\n      popUpShow: false,\n      qrcodeImg: require(\"@/assets/img/qrcode.jpg\"),\n      gameShow: false\n    }\n  },\n  computed: {\n    title: function() {\n      return `您今天还有${this.times}次机会哦`\n    }\n  },\n  methods: {\n    working() {\n      Dialog({ message: '干哈，老老实实等良辰吉日！' });\n    },\n    imagine() {\n      this.show = true;\n    },\n    confirm() {\n      const current_value = parseFloat(this.value);\n      if (current_value === 202155) {\n        Dialog({message: \"bingo~芝麻开门，太聪明了吧，现在可以点击打开啦\"}).then(() => {\n          this.success = true;\n          localStorage.setItem(\"passwordSuccess\", \"1\");\n        });\n      } else {\n        let msg = \"哈哈，您的密码没对上呢~\";\n        if (current_value === 831) {\n          msg = \"就知道你会输入这个，再想想呢~\";\n        } else if (current_value.toString() === \"0503\") {\n          msg = \"哎哟，手机密码也不对呢，再想想~\";\n        } else if (current_value === 121855) {\n          msg = \"居然想到了这条，厉害哟，不过还是得再思考思考~\"\n        }\n        Dialog({ message: msg })\n        this.times = this.times - 1;\n        localStorage.setItem(\n            \"rest\", JSON.stringify({\n              times: this.times,\n              timestamp: new Date().getTime()\n            })\n        );\n      }\n      this.value = \"\";\n    },\n    play() {\n      this.$router.push({\n        name: \"gameList\"\n      })\n    },\n    qrcode() {\n      this.popUpShow = true;\n    }\n  },\n  mounted() {\n    const rest = localStorage.getItem(\"rest\");\n    const rest_data = rest ? JSON.parse(rest) : null;\n    const timestamp = rest_data ? rest_data.timestamp : 0;\n    const current_stamp = new Date().getTime();\n    const times = rest_data ? rest_data.times : 5;\n    const today7 = new Date(new Date().toLocaleDateString()).getTime() + 7 * 60 * 60 * 1000;\n    this.times =\n        current_stamp < today7 ? times : timestamp > today7 ? times : 5;\n    const nowdays = new Date().getTime();\n    const isSuccessTime = nowdays > 1620205505000;\n    this.success =\n        localStorage.getItem(\"passwordSuccess\") ?\n            localStorage.getItem(\"passwordSuccess\") :\n            isSuccessTime ?\n                isSuccessTime :\n                false\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n.display_flex {\n  display: flex;\n  align-items: center;\n  flex-direction: column;\n}\n.not_waiting {\n  margin-top: 15px;\n}\n\n</style>\n"],"sourceRoot":"src/view"}]}